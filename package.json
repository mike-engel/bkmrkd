{
  "name": "bkmrkd",
  "version": "2.4.7",
  "description": "A simple self-hosted bookmark manager written in javascript",
  "main": "server.js",
  "scripts": {
    "test": "NODE_ENV=stage babel-node ./migrations/nameChange.js && npm run compile:minify && standard && TZ=Americas/Denver NODE_ENV=stage babel-node test/index.js | tap-spec",
    "start": "NODE_ENV=development babel-node ./migrations/nameChange.js && babel-node server.js",
    "stage": "NODE_ENV=stage babel-node ./migrations/nameChange.js && NODE_ENV=stage pm2 start ./tmp/server.js --name bkmrkd",
    "stage:restart": "NODE_ENV=stage pm2 restart bkmrkdStage",
    "stage:stop": "NODE_ENV=stage pm2 stop bkmrkdStage",
    "prod": "NODE_ENV=production babel-node ./migrations/nameChange.js && NODE_ENV=production pm2 start ./tmp/server.js --name bkmrkdProd",
    "prod:restart": "NODE_ENV=production pm2 restart bkmrkdProd",
    "prod:stop": "NODE_ENV=production pm2 stop bkmrkdProd",
    "compile:minify": "uglifyjs ./dist/js/main.js -o ./dist/js/main.min.js",
    "compile:server": "babel ./server.js --out-file ./tmp/server.js",
    "compile:helpers": "babel ./helpers --out-dir ./tmp/helpers",
    "compile:src": "babel ./src/js --out-dir ./tmp/src/js",
    "compile:config": "babel ./config/*.js --out-dir ./tmp",
    "compile:routes": "babel ./routes --out-dir ./tmp/routes",
    "createDirectories": "mkdir -p ./tmp &&mkdir -p ./tmp/config &&mkdir -p ./tmp/helpers &&mkdir -p ./tmp/routes",
    "copy:config": "cp ./config/environments.json ./tmp/config/environments.json",
    "postinstall": "npm run createDirectories && npm run copy:config && npm run compile:helpers && npm run compile:config && npm run compile:server && npm run compile:src && npm run compile:routes",
    "snyk-protect": "snyk protect"
  },
  "bin": {
    "bkmrkd": "./bin/cmd.js"
  },
  "engines": {
    "node": ">= 4.2"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mike-engel/bkmrkd.git"
  },
  "keywords": [
    "bkmrkd",
    "bookmarks",
    "bookmark",
    "manager",
    "manager",
    "bookmark"
  ],
  "author": "Mike Engel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mike-engel/bkmrkd/issues"
  },
  "homepage": "https://github.com/mike-engel/bkmrkd",
  "dependencies": {
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.3.13",
    "body-parser": "^1.14.1",
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.0",
    "core-decorators": "^0.12.0",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "helmet": "^1.0.1",
    "history": "^2.0.0",
    "hpp": "^0.2.0",
    "http2": "^3.2.0",
    "lodash.escape": "^4.0.0",
    "lodash.unescape": "^4.0.0",
    "pm2": "^1.0.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.2.0",
    "react-router": "^2.0.0",
    "redux": "^3.2.1",
    "rethinkdb": "^2.2.0",
    "snyk": "^1.11.1",
    "socket.io": "^1.4.3",
    "socket.io-client": "^1.4.3",
    "spdy": "^3.0.0",
    "subarg": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "autoprefixer-core": "^6.0.1",
    "babel": "^6.1.18",
    "babel-core": "^6.4.0",
    "babel-eslint": "^6.0.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babelify": "^7.2.0",
    "brfs": "^1.4.1",
    "browser-sync": "^2.11.0",
    "browserify": "^13.0.0",
    "browserify-hmr": "^0.3.1",
    "bundle-collapser": "^1.2.1",
    "css-mqpacker": "^4.0.0",
    "cssnano": "^3.4.0",
    "cssnext": "^1.8.4",
    "enzyme": "^2.0.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-jsx": "^1.1.0",
    "gulp": "^3.9.0",
    "gulp-csslint": "^0.2.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-if": "^2.0.0",
    "gulp-less": "^3.0.5",
    "gulp-postcss": "^6.0.1",
    "gulp-react": "^3.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.1",
    "invariant": "^2.2.0",
    "phantomjs": "^2.1.2",
    "pixrem": "^3.0.0",
    "postcss-color-rgba-fallback": "^2.0.0",
    "postcss-custom-media": "^5.0.0",
    "postcss-import": "^8.0.2",
    "postcss-nested": "^1.0.0",
    "postcss-vmin": "^2.0.0",
    "react-addons-test-utils": "^0.14.6",
    "react-transform-hmr": "^1.0.1",
    "redux-router": "^1.0.0-beta5",
    "sinon": "^1.17.2",
    "standard": "^6.0.1",
    "stylelint": "^5.0.1",
    "supertest": "^1.1.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0",
    "tape-run": "^2.1.0",
    "uglify-js": "^2.6.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "warning": "^2.1.0",
    "watchify": "^3.6.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist/**/*"
    ]
  },
  "snyk": false
}
