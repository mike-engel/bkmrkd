{
  "name": "bkmrkd",
  "version": "3.0.0",
  "description": "A simple self-hosted bookmark manager written in javascript",
  "main": "./index.js",
  "scripts": {
    "compile": "scripty",
    "compile:elm": "scripty",
    "migrate:up": "scripty",
    "migrate:down": "scripty",
    "report-coverage": "scripty",
    "start": "scripty",
    "test": "scripty",
    "test:coverage": "scripty",
    "test:lint": "scripty",
    "test:unit": "scripty",
    "watch:elm": "scripty"
  },
  "engines": {
    "node": ">= 6.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mike-engel/bkmrkd.git"
  },
  "keywords": [
    "bkmrkd",
    "bookmarks",
    "bookmark",
    "manager"
  ],
  "author": "Mike Engel <mike@mike-engel.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mike-engel/bkmrkd/issues"
  },
  "homepage": "https://github.com/mike-engel/bkmrkd",
  "dependencies": {
    "body-parser": "^1.17.2",
    "bunyan": "^1.8.5",
    "compression": "1.7.0",
    "cookie-parser": "^1.4.0",
    "dotenv": "^4.0.0",
    "ejs": "^2.3.4",
    "elm": "1.2.0",
    "express": "^4.15.3",
    "helmet": "3.6.1",
    "hpp": "^0.2.0",
    "knex": "^0.12.6",
    "le_node": "^1.6.7",
    "pg": "^6.1.0",
    "ramda": "^0.23.0",
    "raven": "^1.1.1",
    "rootpath": "^0.1.2",
    "scripty": "^1.7.1",
    "snyk": "1.30.1",
    "subarg": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "codecov": "^1.0.1",
    "eslint-config-standard": "^6.2.1",
    "google-closure-compiler-js": "^20170124.0.0",
    "mocha": "^3.1.2",
    "nyc": "^10.1.2",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "snazzy": "^6.0.0",
    "standard": "^8.6.0",
    "supertest": "^2.0.1"
  },
  "standard": {
    "ignore": [
      "src",
      "static"
    ],
    "globals": [
      "after",
      "afterEach",
      "before",
      "describe",
      "expect",
      "it",
      "sinon"
    ]
  },
  "nyc": {
    "exclude": [
      "config",
      "node_modules",
      "**/*.test.js"
    ]
  },
  "snyk": false
}