{
  "name": "bkmrkd",
  "version": "2.0.0",
  "description": "A simple self-hosted bookmark manager written in javascript",
  "main": "server.js",
  "scripts": {
    "test": "standard && browserify -t babelify test/index.js | tape-run --browser phantom | tap-spec",
    "start": "babel-node server.js",
    "stage": "NODE_ENV=stage pm2 start index.js --name bkmrkd",
    "stage:restart": "NODE_ENV=stage pm2 restart bkmrkdStage",
    "stage:stop": "NODE_ENV=stage pm2 stop bkmrkdStage",
    "prod": "NODE_ENV=production pm2 start index.js --name bkmrkdProd",
    "prod:restart": "NODE_ENV=production pm2 restart bkmrkdProd",
    "prod:stop": "NODE_ENV=production pm2 stop bkmrkdProd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mike-engel/bkmrkd.git"
  },
  "keywords": [
    "bkmrkd",
    "bookmarks",
    "bookmark",
    "manager",
    "manager",
    "bookmark"
  ],
  "author": "Mike Engel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mike-engel/bkmrkd/issues"
  },
  "homepage": "https://github.com/mike-engel/bkmrkd",
  "dependencies": {
    "babel-cli": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "body-parser": "^1.14.1",
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.0",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "helmet": "^0.15.0",
    "history": "^1.13.1",
    "hpp": "^0.2.0",
    "lodash.escape": "^3.0.0",
    "lodash.unescape": "^3.0.0",
    "pm2": "^0.15.10",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.2",
    "redux": "^3.0.4",
    "rethinkdb": "^2.2.0",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.3.7"
  },
  "devDependencies": {
    "autoprefixer-core": "^6.0.1",
    "babel": "^6.1.18",
    "babel-eslint": "^4.1.5",
    "babel-plugin-react-transform": "^1.1.1",
    "babelify": "^7.2.0",
    "brfs": "^1.4.1",
    "browser-sync": "^2.10.0",
    "browserify": "^12.0.1",
    "browserify-hmr": "^0.3.1",
    "bundle-collapser": "^1.2.1",
    "gulp": "^3.9.0",
    "gulp-csslint": "^0.2.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-if": "^2.0.0",
    "gulp-less": "^3.0.5",
    "gulp-postcss": "^6.0.1",
    "gulp-react": "^3.1.0",
    "gulp-uglify": "^1.5.1",
    "phantomjs": "^1.9.19",
    "pre-commit": "^1.1.2",
    "react-transform-hmr": "^1.0.1",
    "redux-router": "^1.0.0-beta5",
    "standard": "^5.4.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.2.2",
    "tape-run": "^2.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.6.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist/**/*"
    ]
  }
}
