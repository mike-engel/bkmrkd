  },
  "homepage": "https://github.com/mike-engel/bkmrkd",
  "dependencies": {
    "body-parser": "^1.14.1",
    "bunyan": "^1.8.3",
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.0",
    "dotenv": "^2.0.0",
    "ejs": "^2.3.4",
    "express": "^4.14.0",
    "helmet": "^2.0.0",
    "hpp": "^0.2.0",
    "knex": "^0.12.5",
    "le_node": "^1.6.7",
    "pg": "^6.1.0",
    "ramda": "^0.22.1",
    "raven": "^0.12.1",
    "rootpath": "^0.1.2",
    "scripty": "^1.7.0",
    "snyk": "^1.11.1",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "autoprefixer-core": "^6.0.1",
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "codecov": "^1.0.1",
    "css-mqpacker": "^5.0.1",
    "cssnano": "^3.4.0",
    "cssnext": "^1.8.4",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-jsx": "^2.0.0",
    "mocha": "^3.1.2",
    "nyc": "^8.3.1",
    "pixrem": "^3.0.0",
    "postcss-color-rgba-fallback": "^2.0.0",
    "postcss-custom-media": "^5.0.0",
    "postcss-import": "^8.0.2",
    "postcss-nested": "^1.0.0",
    "postcss-vmin": "^2.0.0",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "snazzy": "^5.0.0",
    "standard": "^7.0.1",
    "stylelint": "^6.5.1",
    "supertest": "^1.1.0"
  },
  "standard": {
    "ignore": [
      "src",
      "static"
    ],
    "globals": [
      "after",
      "afterEach",
      "describe",
      "expect",
      "it",
      "sinon"
    ]
  },
  "nyc": {
    "exclude": [
      "config",
      "node_modules",
      "**/*.test.js"
