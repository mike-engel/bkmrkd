{
  "name": "bkmrkd",
  "version": "3.2.0",
  "description": "A simple self-hosted bookmark manager written in javascript",
  "main": "./index.js",
  "scripts": {
    "compile": "scripty",
    "compile:elm": "scripty",
    "dev": "scripty",
    "migrate:up": "scripty",
    "migrate:down": "scripty",
    "report-coverage": "scripty",
    "start": "scripty",
    "test": "npm run test:coverage",
    "test:coverage": "scripty",
    "test:unit": "scripty",
    "watch:elm": "scripty",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": ">= 6.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mike-engel/bkmrkd.git"
  },
  "keywords": [
    "bkmrkd",
    "bookmarks",
    "bookmark",
    "manager"
  ],
  "author": "Mike Engel <mike@mike-engel.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mike-engel/bkmrkd/issues"
  },
  "homepage": "https://github.com/mike-engel/bkmrkd",
  "dependencies": {
    "body-parser": "^1.18.0",
    "bunyan": "^1.8.5",
    "compression": "1.7.0",
    "cookie-parser": "^1.4.0",
    "dotenv": "^4.0.0",
    "ejs": "^2.3.4",
    "elm": "^0.18.0",
    "express": "^4.15.3",
    "helmet": "^3.8.1",
    "hpp": "^0.2.0",
    "knex": "^0.13.0",
    "le_node": "^1.6.7",
    "pg": "^7.3.0",
    "ramda": "^0.24.1",
    "raven": "^2.1.2",
    "rootpath": "^0.1.2",
    "scripty": "^1.7.2",
    "subarg": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chokidar-cli": "^1.2.0",
    "codecov": "^2.3.0",
    "google-closure-compiler-js": "^20170806.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.1.3",
    "mocha": "^3.1.2",
    "nodemon": "^1.12.0",
    "nyc": "^11.2.1",
    "prettier": "^1.6.1",
    "proxyquire": "^1.7.10",
    "sinon": "^3.2.1",
    "sinon-chai": "^2.8.0",
    "snyk": "^1.40.2",
    "supertest": "^3.0.0"
  },
  "nyc": {
    "exclude": [
      "config",
      "node_modules",
      "**/*.test.js"
    ]
  },
  "snyk": false,
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
