{
  "name": "bkmrkd",
  "version": "2.0.0",
  "description": "A simple self-hosted bookmark manager written in javascript",
  "main": "server.js",
  "scripts": {
    "test": "standard",
    "start": "babel-node server.js",
    "stage": "NODE_ENV=stage pm2 start index.js --name bkmrkd",
    "stageRestart": "NODE_ENV=stage pm2 restart bkmrkdStage",
    "stageStop": "NODE_ENV=stage pm2 stop bkmrkdStage",
    "prod": "NODE_ENV=production pm2 start index.js --name bkmrkdProd",
    "prodRestart": "NODE_ENV=production pm2 restart bkmrkdProd",
    "prodStop": "NODE_ENV=production pm2 stop bkmrkdProd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mike-engel/bkmrkd.git"
  },
  "keywords": [
    "bkmrkd",
    "bookmarks",
    "bookmark",
    "manager",
    "manager",
    "bookmark"
  ],
  "author": "Mike Engel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mike-engel/bkmrkd/issues"
  },
  "homepage": "https://github.com/mike-engel/bkmrkd",
  "dependencies": {
    "body-parser": "^1.13.2",
    "compression": "^1.5.1",
    "cookie-parser": "^1.3.5",
    "ejs": "^2.3.2",
    "express": "^4.13.3",
    "helmet": "^0.9.1",
    "hpp": "^0.2.0",
    "lodash.escape": "^3.0.0",
    "lodash.unescape": "^3.0.0",
    "pm2": "^0.14.3",
    "react": "^0.13.3",
    "rethinkdb": "^2.0.2",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "standard": "^4.5.2"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "babel": "^5.6.14",
    "babelify": "^6.1.2",
    "brfs": "^1.4.0",
    "browser-sync": "^2.7.13",
    "browserify": "^10.2.4",
    "bundle-collapser": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-csslint": "^0.1.5",
    "gulp-cssmin": "^0.1.7",
    "gulp-if": "^1.2.5",
    "gulp-less": "^3.0.3",
    "gulp-postcss": "^5.1.9",
    "gulp-react": "^3.0.1",
    "gulp-uglify": "^1.2.0",
    "pre-commit": "^1.0.10",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.2.3"
  }
}
