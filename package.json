    "test:lint": "scripty",
    "test:unit": "scripty",
    "watch:elm": "scripty"
  },
  "engines": {
    "node": ">= 6.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mike-engel/bkmrkd.git"
